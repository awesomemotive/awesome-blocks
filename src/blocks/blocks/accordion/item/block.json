{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "awesome-blocks/accordion-item",
	"version": "0.1.0",
	"title": "Accordion Item",
	"category": "text",
	"description": "Share more content in less space with an accordion that expands and collapses.",
	"keywords": [ "accordions", "collapse", "faq", "awesome" ],
	"parent": [ "awesome-blocks/accordion" ],
	"example": {},
	"attributes": {
		"title": {
			"type": "string",
			"default": ""
		},
		"defaultOpen": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "awesome-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
